# Limpar os conflitos que ainda existem
cat > .gitignore << 'EOF'
# Python
__pycache__
*.pyc
*.pyo
*.pyd
*.so
.python-version

# Environments
.env
.env.local
.venv
env/
venv/
ENV/

# IDE
.vscode/
.idea/
*.swp
*.swo

# Logs
*.log
logs/

# Cache
.cache/
.coverage
htmlcov/

# Distribution
dist/
build/
*.egg-info/

# Docker
.dockerignore

# OS
.DS_Store
Thumbs.db

# Project specific
*.backup
.backup_conflicts/
templates/reports/*.html
EOF'